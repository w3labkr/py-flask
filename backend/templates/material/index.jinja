{% extends '{}/layouts/{}.jinja'.format(settings.theme, settings.layout) %}



{# Head #}
{% block title %}홈{% endblock %}
{% block stylesheet %}
<link rel="stylesheet" href="{{ url_for('static', filename='{}/vendor/slick/1.8.1/slick.css'.format(settings.theme)) }}">
<link rel="stylesheet" href="{{ url_for('static', filename='{}/vendor/slick/1.8.1/slick-theme.css'.format(settings.theme)) }}">
<style>
.main-area > .mdl-grid{ padding: 0; }
.content-area.mdl-cell{ width:100%; margin: 0; }

.section { padding-top: 50px; }
.section h2 {margin-bottom: 0;padding: 0 16px;}
.section .mdl-card {
    justify-content: space-between;
}
</style>
{% endblock %}




{# body #}
{% block hero %}
<div id="hero" class="site-hero vh-75">
  <div class="hero-inner">

    <div class="slider">
      <div class="slides slider-slick">

        <div class="slide aligns" style="background-image: url({{ url_for('static', filename='{}/assets/img/lorem-picsum-01-1920x1024.jpg'.format(settings.theme)) }});">
          <div class="slide-inner align-middle"></div>
        </div>
        <div class="slide aligns" style="background-image: url({{ url_for('static', filename='{}/assets/img/lorem-picsum-02-1920x1024.jpg'.format(settings.theme)) }});">
          <div class="slide-inner align-middle"></div>
        </div>
        <div class="slide aligns" style="background-image: url({{ url_for('static', filename='{}/assets/img/lorem-picsum-03-1920x1024.jpg'.format(settings.theme)) }});">
          <div class="slide-inner align-middle"></div>
        </div>

      </div>{# slides #}
    </div>{# slider #}

  </div>{# hero-inner #}
</div>{# hero #}
{% endblock %}



