{% block script %}
<script src="{{ url_for('static', filename='{}/vendor/slick/1.8.1/slick.min.js'.format(settings.theme)) }}"></script>
<script>
$('.slider-slick').on('init', function(event, slick){
  /**
  * slick kills background-attachment
  * @link https://github.com/kenwheeler/slick/issues/941
  */
  $(this).find(".slick-list").css("transform", "none");
  $(this).find(".slick-track").css("transform", "none");
  // hide slider initialize image
  if ( !! $(this).prev('.slider-initialize').length ) {
    $(this).prev('.slider-initialize').hide();
  }
})
.slick({
  arrows: true,
  dots: false,
  infinite: true,
  slidesToShow: 1,
  slidesToScroll: 1,
  prevArrow : '<button type="button" class="slider-arrow slider-prev">이전</button>',
  nextArrow : '<button type="button" class="slider-arrow slider-next">다음</button>',
  fade: true,
  autoplay: false,
  autoplaySpeed: 3000,
  pauseOnHover: false,
});
</script>
{% endblock %}